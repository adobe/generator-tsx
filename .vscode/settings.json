{
  "editor.formatOnSave": true,
  "files.associations": {
    "_redirects": "ini",
    ".eslintrc.json": "jsonc"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "generators/*.js": true,
    "generators/*.d.ts": true,
    "generators/*/*.js": true,
    "generators/*/*.d.ts": true
  },
  "search.exclude": {
    "**/coverge": true,
    "**/node_modules": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
