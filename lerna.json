{
  "packages": [
    "."
  ],
  "command": {
    "publish": {
      "allowBranch": [
        "master"
      ],
      "commitHooks": false,
      "conventionalCommits": true,
      "ignoreChanges": [
        "*.md",
        "generators/*.test.ts",
        "generators/*/*.test.ts"
      ],
      "message": "chore(release): publish %s"
    }
  },
  "version": "0.1.4"
}
